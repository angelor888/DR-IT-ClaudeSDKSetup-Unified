    "axios": "^1.7.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.4.0",
    "google-auth-library": "^9.14.0",
    "googleapis": "^144.0.0",
    "helmet": "^8.1.0",
