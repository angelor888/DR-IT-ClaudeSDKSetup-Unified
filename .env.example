# DuetRight Dashboard Environment Variables
# Copy this file to .env and fill in the values

# Slack Configuration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_SIGNING_SECRET=your-slack-signing-secret
SLACK_CLIENT_ID=your-slack-client-id
SLACK_CLIENT_SECRET=your-slack-client-secret
SLACK_VERIFICATION_TOKEN=your-slack-verification-token

# Jobber Configuration
JOBBER_CLIENT_ID=your-jobber-client-id
JOBBER_CLIENT_SECRET=your-jobber-client-secret
JOBBER_ACCESS_TOKEN=your-jobber-access-token
JOBBER_REFRESH_TOKEN=your-jobber-refresh-token

# QuickBooks Configuration
QUICKBOOKS_CONSUMER_KEY=your-quickbooks-consumer-key
QUICKBOOKS_CONSUMER_SECRET=your-quickbooks-consumer-secret
QUICKBOOKS_ACCESS_TOKEN=your-quickbooks-access-token
QUICKBOOKS_REFRESH_TOKEN=your-quickbooks-refresh-token
QUICKBOOKS_REALM_ID=your-quickbooks-realm-id
QUICKBOOKS_SANDBOX=false

# Google Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GMAIL_CLIENT_ID=your-gmail-client-id
GMAIL_CLIENT_SECRET=your-gmail-client-secret
GOOGLE_DRIVE_CLIENT_ID=your-google-drive-client-id
GOOGLE_DRIVE_CLIENT_SECRET=your-google-drive-client-secret
GOOGLE_DRIVE_REFRESH_TOKEN=your-google-drive-refresh-token

# Airtable Configuration
AIRTABLE_API_KEY=your-airtable-api-key

# SendGrid Configuration
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=your-sendgrid-from-email

# GitHub Configuration
GITHUB_TOKEN=your-github-token

# Twilio Configuration (if needed)
# TWILIO_ACCOUNT_SID=your-twilio-account-sid
# TWILIO_AUTH_TOKEN=your-twilio-auth-token
# TWILIO_PHONE_NUMBER=your-twilio-phone-number

# Firebase Configuration (will be added when project is created)
# FIREBASE_PROJECT_ID=your-firebase-project-id
# FIREBASE_PRIVATE_KEY=your-firebase-private-key
# FIREBASE_CLIENT_EMAIL=your-firebase-client-email